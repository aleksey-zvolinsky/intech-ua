<script type="text/javascript">
	var page = 0
	var channels = [ #foreach ( $ch in $channels ) $ch.id, #end ]
	function changePage( newpage ) {
		if ( newpage >=0 && newpage < 2 ) {
			page = newpage;
			for ( i=1; i<=10; i++ ) {
				idx = newpage*10 + i - 1;
				if ( idx < channels.length ) {
					$('frame'+i).src='view?id='+channels[idx];
				} else {
					$('frame'+i).src='empty';
				}
			}
		}
	}
</script>

